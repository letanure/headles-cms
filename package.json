    "@commitlint/config-angular": "^8.3.4",
    "core-js": "^3.4.4",
    "element-ui": "^2.4.5",
    "firebase": "^7.8.2",
    "husky": "^4.2.3",
    "lodash-es": "^4.17.15",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-i18n": "^8.15.4",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@qiwi/semantic-release-gh-pages-plugin": "^1.15.8",
    "@semantic-release/changelog": "^5.0.0",
